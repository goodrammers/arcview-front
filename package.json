{
    "name": "arc-view-front",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "scripts": {
        "json-server": "json-server db.json --port 4400 --routes routes.json",
        "vite": "vite",
        "dev": "concurrently \"yarn json-server\" \"yarn vite\"",
        "build": "run-p type-check \"build-only {@}\" --",
        "preview": "vite preview",
        "build-only": "vite build",
        "type-check": "vue-tsc --build",
        "lint": "eslint . --fix",
        "format": "prettier --write src/"
    },
    "dependencies": {
        "@mdi/font": "^7.4.47",
        "axios": "^1.10.0",
        "pinia": "^3.0.3",
        "vue": "^3.5.17",
        "vuetify": "^3.8.11"
    },
    "devDependencies": {
        "@tsconfig/node22": "^22.0.2",
        "@types/node": "^22.15.32",
        "@vitejs/plugin-vue": "^6.0.0",
        "@vue/eslint-config-prettier": "^10.2.0",
        "@vue/eslint-config-typescript": "^14.5.1",
        "@vue/tsconfig": "^0.7.0",
        "concurrently": "^9.2.0",
        "eslint": "8.57.1",
        "eslint-plugin-vue": "~10.2.0",
        "jiti": "^2.4.2",
        "json-server": "0.17.3",
        "npm-run-all2": "^8.0.4",
        "prettier": "3.5.3",
        "sass-embedded": "^1.89.2",
        "typescript": "~5.8.0",
        "vite": "^7.0.0",
        "vue-tsc": "^2.2.10"
    }
}
